	<parameter name="category_age" class="java.lang.String"/>
	<parameter name="search_pengunjung" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
     datapengunjung.`id` AS id,
     datapengunjung.`nama` AS name,
     datapengunjung.`usia` AS u,
     datapengunjung.`category_age` AS age,
     datapengunjung.`alamat` AS almt,
     datapengunjung.`phone` AS phone,
     datapengunjung.`tanggal` AS tgl
FROM
     `datapengunjung` datapengunjung
WHERE
     (
         -- Jika category_age tidak dipilih dan hanya mencari berdasarkan nama
         ($P{category_age} = '-- Category Age --' OR $P{category_age} IS NULL) 
         AND ($P{search_pengunjung} = 'Search Pengunjung' OR nama LIKE CONCAT('%', $P{search_pengunjung}, '%') )
     ) OR (
         -- Jika hanya mencari berdasarkan category_age
         ($P{search_pengunjung} = 'Search Pengunjung' OR $P{search_pengunjung} = '')
         AND $P{category_age} != '-- Category Age --'
         AND category_age = $P{category_age}
     )
     OR   (
         -- Jika category_age dipilih dan mencari berdasarkan nama
         $P{category_age} != '-- Category Age --' 
         AND category_age = $P{category_age} 
         AND nama LIKE CONCAT('%', $P{search_pengunjung}, '%')
     );]]>
	</queryString>
